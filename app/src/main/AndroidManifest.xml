<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thien.footballmanagement">

    <uses-permission android:name="android.permission.INTERNET" />

    <permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LeagueDetail" />
        <activity android:name=".AddLeague" />
        <activity android:name=".AddTournament" />
        <activity android:name=".TournamentDetail" />
        <activity android:name=".AddTeam" />
        <activity android:name=".TeamDetail" />
        <activity android:name=".RoundDetail" />
        <activity android:name=".RankActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".PlayerDetail"></activity>
    </application>

</manifest>